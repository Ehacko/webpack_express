extends layout

block content
  if page === "Recettes"
    -const recettes = []
    -recettes.push({name:'recetteA', url:'/recettes/?name=recetteA'})
    -recettes.push({name:'recetteB', url:'/recettes/?name=recetteB'})
    -recettes.push({name:'recetteC', url:'/recettes/?name=recetteC'})
    -recettes.push({name:'recetteD', url:'/recettes/?name=recetteD'})
    each recette in recettes
      a.W-banner(value=recette.name href=recette.url)
  else
    p welcome